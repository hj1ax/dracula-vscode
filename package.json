{
	"name": "theme-dracula",
	"version": "2.22.4",
	"displayName": "Dracula Sharp",
	"homepage": "https://draculatheme.com/",
	"description": "A Dracula Official Theme fork adapted to my personal preferences.",
	"publisher": "hj1ax",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/dracula.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"maintainers": [
		"Jiaxing Hu <jiaxinghu52@gmail.com>"
	],
	"contributors": [
		"Krzysztof Wisznarewski",
		"Daniel Ramos",
		"Eric Jackson",
		"Elliott Pogue",
		"Ajit Singh",
		"Teddy Bradford"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/hj1ax/dracula-vscode.git"
	},
	"bugs": {
		"url": "https://github.com/hj1ax/dracula-vscode/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dracula",
		"dark",
		"theme",
		"color-theme"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#3c4557",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Dracula Sharp",
				"uiTheme": "vs-dark",
				"path": "./theme/dracula.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^3.14.0",
		"prettier": "^2.2.0",
		"tinycolor2": "^1.4.2",
		"vsce": "^1.81.1"
	}
}
